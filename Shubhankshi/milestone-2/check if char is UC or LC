#include <iostream>
using namespace std;

void check(char ch)
{

    if (ch >= 'A' && ch <= 'Z')
        cout<< ch << " is an UpperCase character\n";

    else if (ch >= 'a' && ch <= 'z')
    cout<< ch << " is an LowerCase character\n";


    else
        cout<< ch << " is not an aplhabetic character\n";

}
  int main(){

      char ch;

      cin>>ch;
      check(ch);

  }


